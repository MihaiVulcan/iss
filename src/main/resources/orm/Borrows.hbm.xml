<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
                "-//Hibernate/Hibernate Mapping DTD//EN"
                "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="Model.Borrow"
       table="Borows">
    <id name="id" type="int" column="id">
        <generator class="increment"/>
    </id>
    <property
            name="bookId"
            column="bookId"/>
    <property
            name="userId"
            column="userId"/>
    <property
            name="startDate"
            column="startData"/>
    <property
            name="endDate"
            column="endDate"/>
    <property
            name="extended"
            column="extended"/>
    <property
            name="returned"
            column="returned"/>

    </class>
</hibernate-mapping>