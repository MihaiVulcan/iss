<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
                "-//Hibernate/Hibernate Mapping DTD//EN"
                "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="Model.User"
       table="Users">
    <id name="cnp" column="cnp">
        </id>
    <property
            name="name"
            column="name"/>
    <property
            name="username"
            column="username"/>
    <property
            name="phone"
            column="phone"/>
    <property
            name="password"
            column="password"/>
    <property
            name="address"
            column="addres"/>

    </class>
</hibernate-mapping>